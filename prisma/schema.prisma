datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          Int      @id @default(autoincrement())
  email       String   @unique
  password    String
  about       String?
  street      String?
  city        String?
  state       String?
  zip         String?
  birthDate   DateTime?
  onboardStep Int      @default(1)
}

model ComponentConfig {
  id         Int    @id @default(autoincrement())
  pageNumber Int
  component  String
}
